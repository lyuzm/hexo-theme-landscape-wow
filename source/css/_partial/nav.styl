.nav
  background: url(nav-bg-url) no-repeat 50% 0
  height: 90px
  width: 100%
  padding-top: 66px
  position: relative
  z-index: 10

  &:after, &:before
    content: ""
    display: block
    position: absolute
    top: 53px
    width: 83px
    height: 76px

  &:before
    background: url(nav-icon-left-url) no-repeat 0 0
    left: 0;

  &:after
    background: url(nav-icon-right-url) no-repeat 0 0
    right: 0

  ul
    display: flex
    align-items: center
    justify-content: center
    height: 64px
    li
      height: 100%
      max-width: 150px
      min-width: 80px
      width: 9%
      &:hover
        span
          color: #fff
      a
        font-size: 18px
        width: 100%
        height: 100%
        display: flex
        align-items: center
        justify-content: center
        transition: .25s all 0s cubic-bezier(.23,1,.32,1)
      span
        background-image: linear-gradient(180deg, #fffdb4,#e8ae29,#ffff82)
        box-decoration-break: clone
        color: transparent
        display: block
        width: 80px
        height: 25px
        background-clip: text
  .logo 
    width: 406px
    max-width: 406px
    height: 162px
    position: relative
    top: 5px
    a
      background: url(nav-logo-url) no-repeat 50% 50%;
      text-indent: -999em;
      background-size: 100% auto;


